/* pages/detail/detail.wxss */

.mainContent {
  width: 100%;
  /* height: 100%; */
  background: #f3f3f3;
  box-sizing: border-box;
  margin-top: 160rpx;
  position: absolute;
  z-index: 888;
  padding: 0 15rpx;
  overflow: scroll;
}

.images01 {
  width: 100%;
  padding: 20rpx 0;
}
.images01 >image{
  border-radius: 10rpx;
}

.introduce {
  width: 55%;
  height: 100rpx;
  margin: 0 auto;
  margin-bottom: 20rpx;
  position: relative;
}

.introduce view {
  width: 100%;
  height: 100%;
  line-height: 110rpx;
  text-align: center;
  color: #fff;
  font-size: 40rpx;
  position: absolute;
  top: 0;
  left: 20rpx;
}

.introduceContent {
  width: 100%;
  background-color: #fff;
  border-radius: 30rpx;
  margin-top: 30rpx;
  padding: 30rpx;
  padding-bottom: 50rpx;
  position: relative;
  z-index: 6;
  text-align: justify;
}

.topD {
  position: absolute;
  left: 10rpx;
  top: 10rpx;
  z-index: -1;
}

.botD {
  position: absolute;
  right: 0rpx;
  bottom: 10rpx;
  z-index: -1;
}

.targets {
  position: relative;
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
}

.targets .introduce {
  margin-top: 30rpx;
  margin-bottom: 20rpx;
}

.decorate {
  width: 20%;
  height: 60rpx;
  position: absolute;
  left: 7%;
  top: 125rpx;
}

.targetsFor {
  width: 100%;
  position: relative;
  padding-bottom: 55rpx;
  display: flex;
  flex-direction: column;
}

.Outfit {
  width: 40%;
  height: 70rpx;
  margin-bottom: 20rpx;
  position: absolute;
  left: 30%;
  z-index: 3;
}

.Outfit view {
  width: 100%;
  height: 100%;
  line-height: 70rpx;
  text-align: center;
  color: #fff;
  font-size: 30rpx;
  position: absolute;
  top: 0;
}

.thought {
  width: 90%;
  color: #4d4d4d;
  font-size: 26rpx;
  margin: 0 auto;
  padding: 60rpx 40rpx;
  background: #fff;
  z-index: 2;
  position: relative;
  top: 35rpx;
  border-radius: 16rpx;
  text-align: justify;
}

.imLeft, .imRight {
  width: 33rpx;
  height: 150rpx;
  position: absolute;
  top: -65rpx;
  z-index: 4;
}

.imLeft {
  left: 15%;
}

.imRight {
  right: 15%;
}
